{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 7
    },
    "rules": {
        "indent": ["error", 4, { "SwitchCase": 1 }],
        "camelcase": "off",
        "comma-dangle": "off",
        "no-bitwise": "off",
        "linebreak-style": "off"
    }
}
